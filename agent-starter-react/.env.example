ADMIN_EMAIL=your-admin-email@example.com

# Enviroment variables needed to connect to the LiveKit server.
LIVEKIT_URL=wss://your-livekit-server.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# ===========================================
# DATABASE CONFIGURATION
# ===========================================
# MongoDB connection string
DATABASE_URL=mongodb://localhost:27017
DB_NAME=agent_starter_db

# ===========================================
# AUTHENTICATION & SECURITY
# ===========================================
# JWT secret key (generate a strong random string)
JWT_SECRET=your_jwt_secret_key_here

# NextAuth secret key (generate a strong random string)
NEXTAUTH_SECRET=your_nextauth_secret_key_here

# Application URL (change for production)
NEXTAUTH_URL=http://localhost:3000

# ===========================================
# EMAIL CONFIGURATION (SMTP)
# ===========================================
# SMTP server settings
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your-email@gmail.com
SMTP_PASS=your_app_password_here
SMTP_FROM=your-email@gmail.com

# ===========================================
# OAUTH PROVIDERS
# ===========================================
# Google OAuth (get from Google Cloud Console)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# GitHub OAuth (get from GitHub Developer Settings)
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# ===========================================
# RATE LIMITING (Optional)
# ===========================================
# Maximum login attempts before lockout
RATE_LIMIT_MAX_ATTEMPTS=5

# Time window for rate limiting (in milliseconds)
RATE_LIMIT_WINDOW_MS=900000

# Lockout duration after exceeding attempts (in milliseconds)
RATE_LIMIT_BLOCK_DURATION_MS=3600000

# ===========================================
# APPLICATION CONFIGURATION
# ===========================================
# Environment: development, production, test
NODE_ENV=development

# Backend API URL
BACKEND_URL=http://localhost:8000
